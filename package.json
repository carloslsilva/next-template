{
  "name": "next-template",
  "description": "A Next.js template using TailwindCSS",
  "author": "@carloslsilva",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "export": "next build && next export",
    "dev": "next dev",
    "lint": "next lint",
    "format": "prettier --write **/*.{js,jsx,json,css,scss,md}",
    "clean": "rimraf .next out",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "clsx": "1.2.1",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.13",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.2",
    "rimraf": "4.1.2",
    "tailwindcss": "3.2.6"
  }
}
